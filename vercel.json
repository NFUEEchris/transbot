{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python",
            "config": {
                "includeFiles": "public/**"
            }
        }
    ],
    "routes": [
        {
          "src": "/(.*)", 
          "dest": "app.py"
        }
    ]

}

